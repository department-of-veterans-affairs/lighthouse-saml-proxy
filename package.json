{
  "name": "saml-proxy",
  "description": "SAML 2.0 Proxy allows modification/mediation of requests/responses",
  "version": "1.1.0",
  "private": false,
  "author": "Department of Veterans Affairs",
  "keywords": [
    "saml",
    "idp",
    "okta"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/department-of-veterans-affairs/lighthouse-saml-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/department-of-veterans-affairs/lighthouse-saml-proxy/issues"
  },
  "main": "./app.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "tsc",
    "watch": "tsc --watch",
    "prestart": "yarn build && npx eslint .",
    "start": "node build/app.js",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "test": "jest --testPathIgnorePatterns saml-proxy-regression.test.js",
    "test:ci": "jest --ci --testPathIgnorePatterns saml-proxy-regression.test.js",
    "test:watch": "jest --watch",
    "start-dev": "npx eslint . && ts-node -r source-map-support/register src/app.js --config dev-config.json",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "prepare": "husky install"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "lines": 83.45
      }
    },
    "transform": {
      ".(js|ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "bin": "./bin/run.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@department-of-veterans-affairs/formation": "^8.0.1-3",
    "@sentry/node": "^6.4.0",
    "@types/express-session": "^1.17.3",
    "@types/lodash.assignin": "^4.2.6",
    "@types/morgan": "^1.9.2",
    "@types/passport": "^1.0.6",
    "@types/redis": "^2.8.28",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "@types/xml2js": "^0.4.8",
    "@xmldom/xmldom": "^0.7.9",
    "acorn": "^8.1.1",
    "body-parser": "^1.19.0",
    "btoa": "^1.2.1",
    "cls-rtracer": "^2.6.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.18.2",
    "express-prom-bundle": "^6.3.6",
    "express-session": "^1.17.1",
    "extend": "^3.0.2",
    "font-awesome": "^4.7.0",
    "hbs": "^4.2.0",
    "lodash.assignin": "^4.2.0",
    "lodash.isstring": "^4.0.1",
    "lodash.omit": "^4.5.0",
    "lodash.template": "^4.5.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-sass-tilde-importer": "^1.0.2",
    "passport": "^0.6.0",
    "passport-wsfed-saml2": "^4.4.0",
    "prom-client": "^13.1.0",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "samlp": "^7.0.1",
    "sass": "1.35.2",
    "uswds": "^1.6.14",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "xml2js": "^0.4.23",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.14.37",
    "@types/samlp": "^1.0.7",
    "@types/xmldom": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "atob": "^2.1.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.2",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^27.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "async": ">=3.2.3",
    "moment": ">=2.29.4",
    "xml-crypto": ">=2.1.3",
    "trim-newlines": ">=3.0.1",
    "normalize-url": "4.5.1",
    "http-signature": ">=1.3.6",
    "json-schema": ">=0.4.0",
    "node-forge": ">=1.0.0",
    "ejs": ">=3.1.8",
    "qs": ">=6.11.0"
  }
}
